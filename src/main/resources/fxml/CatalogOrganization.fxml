<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<GridPane minHeight="-Infinity" minWidth="-Infinity" stylesheets="/css/Button.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane style="-fx-background-color: #4A6C9B" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <children>
                <Label text="Справочник организаций" textAlignment="CENTER" textFill="white">
                    <font>
                        <Font name="Candara" size="18.0" />
                    </font>
                </Label>
            </children>
        </StackPane>
        <TableView fx:id="table" stylesheets="/css/TableView.css" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <columns>
                <TableColumn minWidth="200" prefWidth="210" text="Наименование организации">
                </TableColumn>
                <TableColumn minWidth="200" prefWidth="210" text="Код по ОКПО">
                </TableColumn>
                <TableColumn minWidth="200" prefWidth="210" text="Тип организации">
                </TableColumn>
            </columns>
        </TableView>
        <GridPane alignment="BASELINE_RIGHT" GridPane.columnIndex="0" GridPane.rowIndex="3">
           <children>
               <Button alignment="CENTER_RIGHT" minHeight="30.0" minWidth="120" prefHeight="30" prefWidth="120" text="Применить" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0">
            </Button>
               <Button alignment="CENTER_RIGHT" minHeight="30" minWidth="120" prefHeight="30" prefWidth="120.0" text="Отмена" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="0">
            </Button>
           </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="ALWAYS" />
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="ALWAYS" />
      <RowConstraints />
      <RowConstraints />
    </rowConstraints>
</GridPane>
