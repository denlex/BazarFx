                <Insets left="10.0" right="10.0" />
            </GridPane.margin>
        </ComboBox>
        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="4">
            <children>
            <HBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="19.0">
               <children>
                      <Button alignment="CENTER" mnemonicParsing="false" onAction="#saveButtonClicked" text="Принять" AnchorPane.bottomAnchor="15" AnchorPane.rightAnchor="150" />
                      <Button alignment="CENTER" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Отмена" AnchorPane.bottomAnchor="15" AnchorPane.rightAnchor="15" />
               </children>
            </HBox>
            </children>
            <GridPane.margin>
                <Insets />
            </GridPane.margin>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" />
        <ColumnConstraints hgrow="ALWAYS" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" />
        <RowConstraints minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
        <RowConstraints minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
        <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="ALWAYS" />
        <RowConstraints />
