<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane stylesheets="/css/Button.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane style="-fx-background-color: #4A6C9B" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0">
            <children>
            <Label stylesheets="/css/LabelProperty.css" text="Добавление КО">
            </Label>
            </children>
        </StackPane>
        <Label text="Код" GridPane.columnIndex="0" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
        <TextField maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1">
        </TextField>
        <Label text="Наименование" GridPane.columnIndex="0" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
        <TextField maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2">
        </TextField>
        <TabPane GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="3">
         <tabs>
            <Tab text="Untitled Tab">
               <content>
                  <GridPane>
                      <children>
                          <TableView>
                           <columns>
                              <TableColumn maxWidth="1.7976931348623157E308" minWidth="300.0" prefWidth="120.0" text="Характеристика" />
                              <TableColumn maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="200.0" text="Единица измерения" />
                              <TableColumn maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="150.0" text="Значение" />
                           </columns></TableView>
                      </children>
                    <columnConstraints>
                      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </Tab>
            <Tab text="Загрузка изображения">
               <content>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="ALWAYS" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints vgrow="ALWAYS" />
                        <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
                    </rowConstraints>
                     <children>
                        <ImageView fitHeight="370.0" fitWidth="370.0" pickOnBounds="true" preserveRatio="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                           <GridPane.margin>
                              <Insets left="10.0" right="10.0" top="10.0" />
                           </GridPane.margin>
                           <image>
                              <Image/>
                           </image>
                        </ImageView>
                        <Button alignment="CENTER" mnemonicParsing="false" style="-fx-pref-width: 370px" text="Загрузить изображение" GridPane.rowIndex="1">
                           <GridPane.margin>
                              <Insets bottom="10.0" left="10.0" />
                           </GridPane.margin>
                        </Button>
                     </children>
                  </GridPane>
               </content>
            </Tab>
         </tabs></TabPane>
        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="4">
            <children>
                <Button alignment="CENTER" mnemonicParsing="false" text="Принять" AnchorPane.bottomAnchor="15" AnchorPane.rightAnchor="150" />
                <Button alignment="CENTER" mnemonicParsing="false" text="Отмена" AnchorPane.bottomAnchor="15" AnchorPane.rightAnchor="15" />
            </children>
        </AnchorPane>
    </children>
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" minWidth="110.0" prefWidth="110.0" />
      <ColumnConstraints hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="NEVER" />
      <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
      <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="NEVER" />
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="NEVER" />
   </rowConstraints>

</GridPane>
