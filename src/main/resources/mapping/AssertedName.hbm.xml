<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.defence.domain.entities">
    <class name="AssertedName" table="ASSERTED_NAME">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="code" column="Code" type="string"/>
        <property name="name" column="Name" type="string"/>

        <set name="catalogDescriptions" table="CATALOG_DESCRIPTIONS"
             inverse="false" lazy="false" fetch="select" cascade="all">
            <key column="ASSERTED_NAME_ID" />
            <one-to-many class="CatalogDescription" />
        </set>
    </class>
</hibernate-mapping>