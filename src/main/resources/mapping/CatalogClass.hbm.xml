<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.defence.domain.entities">
    <class name="CatalogClass" table="CatalogClass">
        <id name="id" column="Id">
            <generator class="native"/>
        </id>
        <property name="code" column="Number" type="int"/>
        <property name="name" column="Name" type="string"/>
        <property name="include" column="Include" type="string"/>
        <property name="exclude" column="Exclude" type="string"/>
    </class>
</hibernate-mapping>