<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.defence.domain.entities">
    <class name="RegistrationInfo" table="REGISTRATION_INFO">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>

        <property name="applicationNumber" column="APPLICATION_NUMBER" type="string"/>
        <property name="registrationDate" column="REGISTRATION_DATE" type="date"/>
        <property name="registrationNumber" column="REGISTRATION_NUMBER" type="string"/>
    </class>
</hibernate-mapping>