<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.defence.domain.entities">
    <class name="Value" table="VALUE">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>

        <property name="value" column="VALUE" type="string"/>

        <set name="characteristicKitSet" table="CHARACTERISTIC_KIT_SET"
             inverse="false" lazy="true" fetch="select" cascade="all">
            <key column="VALUE_ID" />
            <many-to-many column="CHARACTERISTIC_KIT_ID" class="CharacteristicKit" />
        </set>
    </class>
</hibernate-mapping>